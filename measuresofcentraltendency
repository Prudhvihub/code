import statistics

list = [9,3,5,7,5,3,1,6]
mean = statistics.mean(list)
print(f"the mean: {mean:.2f}")


median = statistics.median(list)
print(f"median of the values:{medain}")

Mode = statistics.mode(list)
print(f'mode in the values:{Mode}")

WEIGHTED AVERAGE

import numpy as np

values = [4,6,8,2]
value_weights = [1,3,5,7]
weighted_avg = np.average(values, weights=value_weights)
print(f"the weighted ave of value when added weights is :{weighted_avg:.2f}")
